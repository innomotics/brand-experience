{
  "name": "@innomotics/brand-experience-icons",
  "version": "0.0.2-beta-34",
  "description": "> TODO: description",
  "author": "ad001\\z003a0vy <Istvan.Herbak@evosoft.com>",
  "homepage": "https://devops.alm.innomotics.net/in/innomotics-brand-experience/brand-experience#readme",
  "license": "MIT",
  "private": false,
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://devops.alm.innomotics.net/in/innomotics-brand-experience/brand-experience.git"
  },
  "scripts": {
    "build": "npm run generateIconModule && npm run generateBicolorIconModule && npx copy-files-from-to",
    "generateIconModule": "npx ts-node icongenerator.ts",
    "generateBicolorIconModule": "npx ts-node bicoloricongenerator.ts"
  },
  "bugs": {
    "url": "https://devops.alm.innomotics.net/in/innomotics-brand-experience/brand-experience/-/issues"
  },
  "devDependencies": {
    "copy-files-from-to": "^3.12.0",
    "ts-node": "^10.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@iconify/tools": "^4.0.7",
    "jszip": "^3.10.1"
  },
  "gitHead": "258791c1cbf11ee4ff207f0034abe74bcea86955",
  "copyFiles": [
    {
      "from": [
        "./**",
        "!./dist",
        "!./downloadoutput",
        "!./lib",
        "!./node_modules",
        "!./*.*",
        "!./bicolor/*.md",
        "!.svg/*.md"
      ],
      "to": "./dist"
    },
    {
      "from": [
        "./lib/*"
      ],
      "to": "./dist"
    },
    {
      "from": [
        "./svg/readme.md"
      ],
      "to": "../webdocs/docs/icons/innoicons"
    },
    {
      "from": [
        "./bicolor/readme.md"
      ],
      "to": "../webdocs/docs/icons/bicolor"
    }
  ],
  "copyFilesSettings": {
    "ignoreDotFilesAndFolders": true,
    "whenFileExists": "overwrite"
  }
}
