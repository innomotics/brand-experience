  "scripts": {
    "build": "npm run generateIconModule && npm run copyoutputs && npm run copyicons && npm run copyreadme && npm run publishartifacts",
    "copyoutputs": "npx cpx \"./lib/**\" \"./dist/\" && npx cpx \"./svg/**\" \"./dist/svg\" && npx cpx \"./styles/**\" \"./dist/styles\"",
    "generateIconModule": "npx ts-node generator.ts",
    "copyreadme": "npx cpx \"readme.md\" \"../webdocs/docs/icons\"",
    "copyicons": "npx cpx  \"./downloadoutput/**\" \"../webdocs/static/svg\"",
    "publishartifacts": "npx cpx \"./*.md\" \"./dist\""
  },
  "bugs": {
    "url": "https://github.com/innomotics/brand-experience/issues"
  },
