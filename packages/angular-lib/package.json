{
  "name": "@innomotics/brand-experience-angular-lib",
  "version": "0.0.2-beta-25",
  "description": "> TODO: description",
  "author": "",
  "types": "./index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "dist/"
  ],
  "private": false,
  "scripts": {
    "build": "ng-packagr -p ng-package.json -c tsconfig.json && npx copy-files-from-to"
  },
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "esm": "./dist/esm2022/innomotics-brand-experience-angular-lib.mjs",
      "esm2022": "./dist/esm2022/innomotics-brand-experience-angular-lib.mjs",
      "fesm2022": "./dist/fesm2022/innomotics-brand-experience-angular-lib.mjs",
      "default": "./dist/fesm2022/innomotics-brand-experience-angular-lib.mjs"
    }
  },
  "dependencies": {
    "@innomotics/brand-experience": "0.0.2-beta-25"
  },
  "devDependencies": {
    "copy-files-from-to": "^3.12.0",
    "ng-packagr": "^17.3.0 || ^18.0.0"
  },
  "peerDependencies": {
    "@angular/common": "^17.3.5 || ^18.0.0",
    "@angular/core": "^17.3.5 || ^18.0.0",
    "@angular/forms": "^17.3.5 || ^18.0.0",
    "@innomotics/brand-experience": "0.0.2-beta-25",
    "@innomotics/brand-experience-fonts": "0.0.2-beta-25",
    "@innomotics/brand-experience-icons": "^0.0.2-beta-25",
    "typescript": "5.4.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "gitHead": "258791c1cbf11ee4ff207f0034abe74bcea86955",
  "copyFiles": [
    {
      "from": [
        "./lib/package.json",
        "./lib/*.md"
      ],
      "to": "./dist"
    }
  ],
  "copyFilesSettings": {
    "ignoreDotFilesAndFolders": true,
    "whenFileExists": "overwrite"
  }
}
