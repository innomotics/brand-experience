{
  "name": "@innomotics/brand-experience",
  "version": "0.0.2-beta-19",
  "description": "Stencil Component Starter",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "./dist/esm/index.js",
  "es2017": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/innomotics-brand-experience/innomotics-brand-experience.esm.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "./dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "createadditionalreadmes": "npx ts-node colorreadmegenerator.ts && npx ts-node gridlayoutreadmegenerator.ts",
    "build": "stencil build && npm run createadditionalreadmes && npx copy-files-from-to",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "stencil": "stencil"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.11",
    "@innomotics/brand-experience-fonts": "^0.0.2-beta-19",
    "@innomotics/brand-experience-icons": "^0.0.2-beta-19",
    "@stencil/core": "^4.21.0",
    "@stencil/sass": "^3.0.12",
    "animejs": "^3.2.2",
    "luxon": "3.4.4",
    "resize-observer-polyfill": "^1.5.1",
    "sanitize-html": "^2.13.0",
    "simplebar": "^6.2.6"
  },
  "devDependencies": {
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/angular-output-target": "0.9.0",
    "@stencil/react-output-target": "^0.7.1",
    "@stencil/vue-output-target": "^0.8.9",
    "@types/autoprefixer": "^10.2.0",
    "@types/jest": "^29.5.6",
    "@types/luxon": "^3.4.2",
    "@types/node": "^16.18.11",
    "@types/sanitize-html": "^2.11.0",
    "autoprefixer": "^10.4.17",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "peerDependencies": {
    "@innomotics/brand-experience-fonts": "^0.0.2-beta-19",
    "@innomotics/brand-experience-icons": "^0.0.2-beta-19"
  },
  "license": "MIT",
  "gitHead": "1822534a8818dd0f162c03d6b2eb4f22bd8ba914",
  "copyFiles": [
    {
      "from": [
        "./src/components/**/*.md",
        "!./src/components/inno-breadcrumb-item",
        "!./src/components/inno-date-picker-dropdown",
        "!./src/components/inno-footer-item",
        "!./src/components/inno-modal-content",
        "!./src/components/inno-modal-footer",
        "!./src/components/inno-modal-header",
        "!./src/components/inno-select-item",
        "!./src/components/inno-split-gutter",
        "!./src/components/inno-split-item",
        "!./src/components/inno-status-message-container",
        "!./src/components/inno-tab-item"
      ],
      "to": "../webdocs/docs/components"
    },
    {
      "from":["./src/components/inno-breadcrumb-item/*.md"],
      "to":"../webdocs/docs/components/inno-breadcrumb/inno-breadcrumb-item"
    },
    {
      "from":["./src/components/inno-date-picker-dropdown/*.md"],
      "to":"../webdocs/docs/components/inno-date-picker/inno-date-picker-dropdown"
    },
    {
      "from":["./src/components/inno-footer-item/*.md"],
      "to":"../webdocs/docs/components/inno-footer/inno-footer-item"
    },
    {
      "from":["./src/components/inno-modal-content/*.md"],
      "to":"../webdocs/docs/components/inno-modal/inno-modal-content"
    },
    {
      "from":["./src/components/inno-modal-footer/*.md"],
      "to":"../webdocs/docs/components/inno-modal/inno-modal-footer"
    },
    {
      "from":["./src/components/inno-modal-header/*.md"],
      "to":"../webdocs/docs/components/inno-modal/inno-modal-header"
    },
    {
      "from":["./src/components/inno-select-item/*.md"],
      "to":"../webdocs/docs/components/inno-select/inno-select-item"
    },
    {
      "from":["./src/components/inno-split-gutter/*.md"],
      "to":"../webdocs/docs/components/inno-split/inno-split-gutter"
    },
    {
      "from":["./src/components/inno-split-item/*.md"],
      "to":"../webdocs/docs/components/inno-split/inno-split-item"
    },
    {
      "from":["./src/components/status-message-container/*.md"],
      "to":"../webdocs/docs/components/inno-status-message/status-message-container"
    },
    {
      "from":["./src/components/inno-tab-item/*.md"],
      "to":"../webdocs/docs/components/inno-tab/inno-tab-item"
    },
    {
      "from": "./readmes/**/*.md",
      "to": "../webdocs/docs"
    },
    {
      "from": [
        "./**",
        "!./cjs",
        "!./collection",
        "!./components",
        "!./dist",
        "!./esm",
        "!./innomotics-brand-experience",
        "!./lib",
        "!./loader",
        "!./node_modules",
        "!./readmes",
        "!./src",
        "!./types",
        "!./www",
        "!./**/*.*"
      ],
      "to": "./kuka"
    },
    {
      "from": "./lib/**",
      "to": "./dist"
    },
    {
      "from": [
        "./dist/**",
        "!./dist/styles",
        "!./dist/*.json",
        "!./dist/*.md"
      ],
      "to": "./"
    }
  ],
  "copyFilesSettings": {
    "ignoreDotFilesAndFolders": true
  }
}