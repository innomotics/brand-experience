{
  "name": "@innomotics/brand-experience-fonts",
  "version": "0.0.2-beta-28",
  "description": "> TODO: description",
  "author": "ad001\\z003a0vy <Istvan.Herbak@evosoft.com>",
  "homepage": "",
  "license": "MIT",
  "private": false,
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/innomotics/brand-experience"
  },
  "scripts": {
    "build": "npx copy-files-from-to"
  },
  "devDependencies": {
    "copy-files-from-to": "^3.12.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "gitHead": "258791c1cbf11ee4ff207f0034abe74bcea86955",
  "copyFiles": [
    {
      "from": [
        "./**",
        "!./dist",
        "!./lib",
        "!./*.*"
      ],
      "to": "./dist"
    },
    {
      "from": [
        "./lib/**"
      ],
      "to": "./dist"
    },
    {
      "from": [
        "readme.md"
      ],
      "to": "../webdocs/docs/fonts"
    },
    {
      "from": [
        "./fonts/**"
      ],
      "to": "../webdocs/src/fonts"
    }
  ],
  "copyFilesSettings": {
    "ignoreDotFilesAndFolders": true,
    "whenFileExists": "overwrite"
  }
}
