  "author": "ad001\\z003a0vy <Istvan.Herbak@evosoft.com>",
  "homepage": "",
  "license": "MIT",
  "private": false,
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "npx copy-files-from-to"
  },
  "devDependencies": {
    "copy-files-from-to": "^3.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "15d0dae0d9741edd8f4cec2e069acec02081e1d5",
  "copyFiles": [
    {
      "from": [
        "./**",
        "!./dist",
        "!./lib",
        "!./*.*"
      ],
      "to": "./dist"
    },
    {
      "from": [
        "./lib/**"
      ],
      "to": "./dist"
    },
    {
      "from": [
        "readme.md"
      ],
      "to": "../webdocs/docs/fonts"
    },
    {
      "from": [
        "./fonts/**"
      ],
      "to": "../webdocs/src/fonts"
    }
  ],
  "copyFilesSettings": {
    "ignoreDotFilesAndFolders": true,
    "whenFileExists": "overwrite"
  }
}
